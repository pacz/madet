<h1>New sucursal</h1>

<% form_for([@empresa,@sucursal]) do |f| %>
  <%= f.error_messages %>

  
  <p>
    <%= f.label :ciudad %><br />
    <%= f.text_field :ciudad %>
  </p>
  <p>
    <%= f.label :direccion %><br />
    <%= f.text_field :direccion %>
  </p>
  <p>
    <%= f.label :administrador %><br />
    <%= f.text_field :administrador %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', empresa_sucursales_path(@empresa) %>